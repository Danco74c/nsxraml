<edge>
    <id>mandatory</id>
    <description>mandatory</description>
    <datacenterMoid>mandatory</datacenterMoid>
    <name>mandatory</name>
    <fqdn>mandatory</fqdn>
    <enableAesni>true|false</enableAesni>
    <enableFips>true|false</enableFips>
    <vseLogLevel>info</vseLogLevel>
    <vnics>
        <vnic>
            <index>0</index>
            <name>mandatory</name>
            <type>mandatory</type>
            <portgroupId>mandatory</portgroupId>
            <addressGroups>
                <addressGroup>
                    <primaryAddress>mandatory</primaryAddress>
                    <secondaryAddresses>
                        <ipAddress>optional</ipAddress>
                    </secondaryAddresses>
                    <subnetMask>mandatory</subnetMask>
                </addressGroup>
            <addressGroup>
            <mtu>mandatory</mtu>
            <enableProxyArp>true|false</enableProxyArp>
            <enableSendRedirects>true|false</enableSendRedirects>
            <isConnected>true|false</isConnected>
            <inShapingPolicy>
                <averageBandwidth>optional</averageBandwidth>
                <peakBandwidth>optional</peakBandwidth>
                <burstSize>optional</burstSize>
                <enabled>true|false</enabled>
                <inherited>true|false</inherited>
            </inShapingPolicy>
            <outShapingPolicy>
                <averageBandwidth>optional</averageBandwidth>
                <peakBandwidth>optional</peakBandwidth>
                <burstSize>optional</burstSize>
                <enabled>true|false</enabled>
                <inherited>true|false</inherited>
            </outShapingPolicy>
        </vnic>
    </vnics>
    <appliances>
        <applianceSize>optional</applianceSize>
        <appliance>
            <resourcePoolId>mandatory</resourcePoolId>
            <datastoreId>mandatory</datastoreId>
            <vmFolderId>optional</vmFolderId>
        </appliance>
    </appliances>
    <cliSettings>
        <remoteAccess>true|false</remoteAccess>
        <userName>mandatory</userName>
    </cliSettings>
    <features>
        <firewall>
            <defaultPolicy>
                <action>mandatory</action>
                <loggingEnabled>true|false</loggingEnabled>
            </defaultPolicy>
            <rules>
                <rule>
                    <id>mandatory</id>
                    <ruleTag>mandatory</ruleTag>
                    <name>mandatory</name>
                    <ruleType>mandatory</ruleType>
                    <source>
                        <exclude>true|false</exclude>
                        <groupingObjectId>mandatory</groupingObjectId>
                    </source>
                    <destination><destination/>
                    <application>
                        <applicationId>mandatory</applicationId>
                    </application>
                    <action>accept</action>
                    <enabled>true|false</enabled>
                    <loggingEnabled>true|false</loggingEnabled>
                    <matchTranslated>true|false</matchTranslated>
                </rule>
            </rules>
        </firewall>
        <routing>
            <staticRouting>
                <defaultRoute>
                    <vnic>mandatory</vnic>
                    <gatewayAddress>mandatory</gatewayAddress>
                    <description>optional</description>
                </defaultRoute>
                <staticRoutes>
                    <route>
                        <vnic>mandatory</vnic>
                        <network>mandatory</network>
                        <nextHop>mandatory</nextHop>
                        <type>mandatory</type>
                    </route>
                </staticRoutes>
            </staticRouting>
            <ospf>
                <enabled>true|false</enabled>
            </ospf>
        </routing>
        <highAvailability>
            <enabled>true|false</enabled>
            <declareDeadTime>mandatory</declareDeadTime>
            <logging>
                <enable>true|false</enable>
                <logLevel>mandatory</logLevel>
            </logging>
        </highAvailability>
        <syslog>
            <protocol>tcp|udp|tls</protocol>
            <serverAddresses>
                <ipAddress>mandatory</ipAddress>
                <ipAddress>1.1.1.2</ipAddress>
            </serverAddresses>
        </syslog>
        <ipsec>
            <enabled>true|false</enabled>
            <logging>
                <enable>true|false</enable>
                <logLevel>mandatory</logLevel>
            </logging>
            <sites>
                <site>
                    <enabled>true|false</enabled>
                    <name>mandatory</name>
                    <localId>mandatory</localId>
                    <localIp>mandatory</localIp>
                    <peerId>mandatory</peerId>
                    <encryptionAlgorithm>mandatory</encryptionAlgorithm>
                    <mtu>mandatory</mtu>
                    <enablePfs>true|false</enablePfs>
                    <dhGroup>mandatory</dhGroup>
                    <localSubnets>
                        <subnet>mandatory</subnet>
                    </localSubnets>
                    <peerSubnets>
                        <subnet>mandatory</subnet>
                    </peerSubnets>
                    <psk>mandatory</psk>
                    <authenticationMode>mandatory</authenticationMode>
                </site>
            </sites>
            <global>
                <caCertificates/>
                <crlCertificates/>
            </global>
        </ipsec>
        <dhcp>
            <enabled>true|false</enabled>
            <staticBindings>
                <staticBinding>
                    <autoConfigureDNS>true|false</autoConfigureDNS>
                    <bindingId>mandatory</bindingId>
                    <vmId>mandatory</vmId>
                    <vnicId>mandatory</vnicId>
                    <hostname>mandatory</hostname>
                    <ipAddress>mandatory</ipAddress>
                    <defaultGateway>mandatory</defaultGateway>
                    <leaseTime>86400</leaseTime>
                </staticBinding>
            </staticBindings>
            <ipPools>
                <ipPool>
                    <autoConfigureDNS>true|false</autoConfigureDNS>
                    <poolId>mandatory</poolId>
                    <ipRange>mandatory</ipRange>
                    <defaultGateway>mandatory</defaultGateway>
                    <leaseTime>86400</leaseTime>
                </ipPool>
            </ipPools>
            <logging>
                <enable>true|false</enable>
                <logLevel>mandatory</logLevel>
            </logging>
        </dhcp>
        <nat>
            <natRules>
                <natRule>
                    <ruleId>mandatory</ruleId>
                    <ruleTag>mandatory</ruleTag>
                    <ruleType>mandatory</ruleType>
                    <action>mandatory</action>
                    <vnic>mandatory</vnic>
                    <originalAddress>mandatory</originalAddress>
                    <translatedAddress>mandatory</translatedAddress>
                    <loggingEnabled>true|false</loggingEnabled>
                    <enabled>true|false</enabled>
                    <protocol>tcp</protocol>
                    <originalPort>mandatory</originalPort>
                    <translatedPort>mandatory</translatedPort>
                </natRule>
            </natRules>
        </nat>
    </features>
    <autoConfiguration>
        <enabled>true|false</enabled>
        <rulePriority>high</rulePriority>
    </autoConfiguration>
</edge>
