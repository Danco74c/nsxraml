<edge>
    <datacenterMoid>mandatory</datacenterMoid>
    <tenant/>
    <name/>
    <fqdn/>
    <enableAesni/>
    <enableFips/>
    <vseLogLevel/>
    <vnics>
        <vnic>
            <label/>
            <name/>
            <addressGroups>
                <addressGroup>
                    <primaryAddress>mandatory</primaryAddress>
                    <secondaryAddresses>
                        <ipAddress/>
                        <ipAddress/>
                    </secondaryAddresses>
                    <subnetMask>mandatory</subnetMask>
                </addressGroup>
                <addressGroup>
                    <primaryAddress>mandatory</primaryAddress>
                    <secondaryAddresses>
                        <ipAddress/>
                        <ipAddress/>
                    </secondaryAddresses>
                    <subnetMask>mandatory</subnetMask>
                </addressGroup>
            </addressGroups>
            <mtu/>
            <type/>
            <isConnected>true</isConnected>
            <index>0</index>
            <portgroupId>mandatoy</portgroupId>
            <enableProxyArp/>
            <enableSendRedirects/>
        </vnic>
        <vnic>
            <label/>
            <name/>
            <addressGroups>
                <addressGroup>
                    <primaryAddress>mandatory</primaryAddress>
                    <secondaryAddresses>
                        <ipAddress/>
                        <ipAddress/>
                    </secondaryAddresses>
                    <subnetMask>mandatory</subnetMask>
                </addressGroup>
                <addressGroup>
                    <primaryAddress>mandatory</primaryAddress>
                    <secondaryAddresses>
                        <ipAddress/>
                        <ipAddress/>
                    </secondaryAddresses>
                    <subnetMask>mandatory</subnetMask>
                </addressGroup>
            </addressGroups>
            <mtu/>
            <type/>
            <isConnected>true</isConnected>
            <index>1</index>
            <portgroupId>mandatoy</portgroupId>
            <enableProxyArp/>
            <enableSendRedirects/>
        </vnic>
    </vnics>
    <appliances> 
        <applianceSize/>
        <enableCoreDump/>
        <appliance>
            <resourcePoolId>mandatory</resourcePoolId>
            <datastoreId>mandatory</datastoreId>
            <hostId/>
            <vmFolderId/>
            <cpuReservation>
                <limit/>
                <reservation/>
                <shares/>
            </cpuReservation>
            <memoryReservation>
                <limit/>
                <reservation/>
                <shares/>
            </memoryReservation>
        </appliance>
    </appliances>
    <cliSettings>
        <remoteAccess/>
        <userName>mandatory</userName>
        <password>mandatory</password>
        <sshLoginBannerText/>
        <passwordExpiry/>
    </cliSettings>
    <autoConfiguration>
        <enabled/>
        <rulePriority/>
    </autoConfiguration>
    <type>mandatory</type>
    <hypervisorAssist/>
    <edgeAssistId/>
    <dnsClient>
        <primaryDns/>
        <secondaryDns/>
        <domainName/>
        <domainName/>
    </dnsClient>
    <queryDaemon>
        <enabled/>
        <port/>
    </queryDaemon>
</edge>