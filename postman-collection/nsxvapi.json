n        <groupingObjectId>ipset-128</groupingObjectId>  \n        <ipAddress>192.168.10.0/24</ipAddress>  \n      </destination>\n      <application>  \n        <applicationId>application-155</applicationId>  \n        <service>  \n          <protocol>tcp</protocol>\n          <port>80</port>  \n          <sourcePort>1500</sourcePort>  \n        </service>\n      </application>\n      <matchTranslated>true</matchTranslated>  \n      <direction>in</direction>  \n      <action>accept</action>  \n      <enabled>true</enabled>  \n      <loggingEnabled>true</loggingEnabled>  \n      <description>comments</description>  \n    </firewallRule>\n  </firewallRules>\n</firewall>\n"},{"collectionId":"b3a31be1-9f1e-4705-863d-a40bd62c13cc","dataMode":"params","descriptionFormat":"html","description":"Delete NSX Edge firewall configuration.\n\nParameters:\n\n","data":[],"headers":"","id":"0f2607b1-27ff-4d0f-a228-53c6260cd4fc","method":"delete","name":"nsxEdgeFirewallConfigDelete /4.0/edges/{{edgeId}}/firewall/config","preRequestScript":"","pathVariables":{},"responses":[],"synced":false,"tests":"","time":1522964106471,"url":"https://{{nsxmanager}}/api/4.0/edges/{{edgeId}}/firewall/config"},{"collectionId":"b3a31be1-9f1e-4705-863d-a40bd62c13cc","dataMode":"raw","descriptionFormat":"html","description":"Add one or more rules. You can add a rule above a specific rule\nusing the query parameter, indicating the desired ruleID.\n\n\nParameters:\n\n","data":[],"headers":"Content-Type: application/xml\n","id":"edbd74b9-8409-4a19-a437-6f73e225f29c","method":"post","name":"firewallRulesCreate /4.0/edges/{{edgeId}}/firewall/config/rules","preRequestScript":"","pathVariables":{},"responses":[],"synced":false,"tests":"","time":1522964106471,"url":"https://{{nsxmanager}}/api/4.0/edges/{{edgeId}}/firewall/config/rules?aboveRuleId=","rawModeData":"<firewallRules>\n  <firewallRule>\n    <ruleTag></ruleTag>\n    <name></name>\n    <source>\n      <ipAddress></ipAddress>\n      <groupingObjectId></